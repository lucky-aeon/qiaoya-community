    {
      "type": "stat",
      "title": "App Up",
      "id": 1,
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 6 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "up{job=\"app\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP QPS by URI",
      "id": 2,
      "gridPos": { "x": 6, "y": 0, "w": 18, "h": 8 },
