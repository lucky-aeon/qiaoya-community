{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "-- Grafana --",
          "uid": "grafana"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Logs in last 5m",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "sum(count_over_time({job=\"docker\"}[5m]))",
          "refId": "A"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "type": "timeSeries",
      "title": "ERROR rate by service (5m)",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "sum by (service)(count_over_time({level=\"ERROR\"}[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 4 }
    },
    {
      "type": "logs",
      "title": "Recent ERROR logs",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "{level=\"ERROR\"}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 14, "w": 24, "x": 0, "y": 14 },
      "options": { "showLabels": true, "showTime": true }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["loki", "logs"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Loki Overview",
  "uid": "loki_overview"
}

